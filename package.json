{
  "name": "@craftlions/pdfium-rs",
  "version": "0.0.10",
  "repository": {
    "type": "git",
    "url": "https://github.com/craftlions/rs"
  },
  "type": "module",
  "description": "My napi-rs addon",
  "main": "index.js",
  "exports": {
    "types": "./index.d.ts",
    "default": "./index.js"
  },
  "keywords": [
    "napi-rs",
    "NAPI",
    "N-API",
    "Rust",
    "node-addon",
    "node-addon-api"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "provenance": true
  },
  "napi": {
    "binaryName": "pdfium",
    "targets": [
      "aarch64-apple-darwin",
      "x86_64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "x86_64-pc-windows-msvc"
    ]
  },
  "scripts": {
    "prepublishOnly": "napi prepublish -t npm --no-gh-release",
    "test": "node test.mjs"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "npm",
    "README.md",
    "LICENSE*"
  ],
  "devDependencies": {
    "@changesets/cli": "2.29.8",
    "@napi-rs/cli": "3.5.1",
    "@types/node": "24.10.4",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.28.2"
}
